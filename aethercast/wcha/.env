# WCHA_SEARCH_MAX_RESULTS: Max number of search results to fetch and process for a topic.
WCHA_SEARCH_MAX_RESULTS=3

# WCHA_REQUEST_TIMEOUT: Timeout in seconds for fetching content from a URL.
WCHA_REQUEST_TIMEOUT=10

# WCHA_USER_AGENT: User-Agent string for HTTP requests made by WCHA.
WCHA_USER_AGENT=AethercastContentHarvester/0.2
