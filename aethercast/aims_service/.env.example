# Service host and port
AIMS_HOST=0.0.0.0
AIMS_PORT=8000

# Flask debug mode
FLASK_DEBUG=True

# --- Google Cloud Vertex AI Configuration ---
# Path to the Google Cloud service account JSON key file.
# This file needs to be accessible by the Docker container.
# Example: GOOGLE_APPLICATION_CREDENTIALS=./path/to/your-gcp-service-account-key.json
# GOOGLE_APPLICATION_CREDENTIALS=

# Google Cloud Project ID
# GCP_PROJECT_ID=your-gcp-project-id

# Google Cloud Location (e.g., us-central1, europe-west1)
# GCP_LOCATION=us-central1

# Default Google LLM Model ID for AIMS (e.g., gemini-1.0-pro, gemini-1.5-pro-preview-0409)
# AIMS_GOOGLE_LLM_MODEL_ID=gemini-1.0-pro
