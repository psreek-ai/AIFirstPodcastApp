# ASF_SECRET_KEY: Secret key for Flask session management. Change this in production!
# If not set, a temporary UUID will be generated at startup (check logs).
ASF_SECRET_KEY=your_very_secret_key_here_or_generate_one

# ASF_CORS_ALLOWED_ORIGINS: Comma-separated list of allowed origins for SocketIO, or "*" for all.
ASF_CORS_ALLOWED_ORIGINS=*

# ASF_CHUNK_SIZE: Size of audio chunks (in bytes) for streaming.
ASF_CHUNK_SIZE=4096

# ASF_STREAM_SLEEP_INTERVAL: Sleep interval (in seconds) between sending audio chunks.
ASF_STREAM_SLEEP_INTERVAL=0.01

# Flask app and SocketIO server parameters
ASF_HOST=0.0.0.0
ASF_PORT=5006
ASF_DEBUG_MODE=True
